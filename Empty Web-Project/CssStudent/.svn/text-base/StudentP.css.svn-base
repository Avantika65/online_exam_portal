}
.labelLogin {
	PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
.labelPassword {
	PADDING-BOTTOM: 0px; MARGIN: 0px 0px 0px 23px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; PADDING-TOP: 0px
}
INPUT.btn {
	BORDER-BOTTOM: #3887b5 1px solid; BORDER-LEFT: #3887b5 1px solid; BACKGROUND-COLOR: #79b6d6; FONT-STYLE: normal; FONT-FAMILY: trebuchet ms, helvetica, sans-serif; COLOR: #333; FONT-SIZE: 13px; BORDER-TOP: #3887b5 1px solid; FONT-WEIGHT: bold; BORDER-RIGHT: #3887b5 1px solid
}

#footer H3 {
	TEXT-TRANSFORM: uppercase
}
#footer_top {
	BACKGROUND-COLOR: #fff
}
#footer P {
	TEXT-ALIGN: center; PADDING-BOTTOM: 0px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; FONT: 9px Arial, Helvetica, Verdana, sans-serif; COLOR: #4a4646; VERTICAL-ALIGN: middle; PADDING-TOP: 10px
}
.txt {
	border: 1px solid #248cbe;

	TEXT-ALIGN: left; 	BACKGROUND-POSITION: left center; 		COLOR:Black; 	FONT-SIZE: 11px; 	FONT-WEIGHT:normal; 	font-family:arial;
	margin-left: 0px;
 
