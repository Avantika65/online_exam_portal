	MARGIN-TOP: 0px
}
H3 {
	MARGIN-TOP: 0px
}
H4 {
	MARGIN-TOP: 0px
}
H5 {
	MARGIN-TOP: 0px
}
H6 {
	MARGIN-TOP: 0px
}
#wrapper {
	MARGIN: 0px auto
}
#header {
	POSITION: relative; MARGIN: 0px auto; WIDTH: 950px; BACKGROUND: url(CssStudent/1.jpeg) no-repeat center bottom; HEIGHT: 96px
}
#middle {
	PADDING-BOTTOM: 0px; MARGIN: 0px auto; PADDING-LEFT: 0px; WIDTH: 950px; PADDING-RIGHT: 0px; HEIGHT: 1%; PADDING-TOP: 14px
}
#main_home {
	WIDTH: 830px; OVERFLOW: auto
}
#main_home .contentWrapper #body {
	PADDING-BOTTOM: 16px; MARGIN: 0px; PADDING-LEFT: 16px; PADDING-RIGHT: 0px; OVERFLOW: auto; PADDING-TOP: 50px
}
#main_home .contentWrapper #body .contentSecurity {
	PADDING-LEFT: 310px; WIDTH: 250px; FLOAT: left; MARGIN-RIGHT: 0px
}
#main_home .contentWrapper #body .explanation {
	FONT: 12px Arial, Helvetica, Verdana, sans-serif; COLOR: #333
}
#main_home .contentWrapper #body .explanation A:link {
	COLOR: #003399; TEXT-DECORATION: none
}
#main_home .contentWrapper #body .explanation A:visited {
	COLOR: #003399; TEXT-DECORATION: none
}
#main_home .contentWrapper #body .explanation A:hover {
	COLOR: #003399; TEXT-DECORATION: underline
}
#main_home .contentWrapper #body .contentSecurity H1 {
	PADDING-BOTTOM: 5px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; FONT: bold 14px Arial, Helvetica, Verdana, sans-serif; COLOR: #333; PADDING-TOP: 0px
}
#main_home .contentWrapper #body .contentLoggingIn {
	MARGIN-RIGHT: 0px
}
