	PADDING-LEFT: 310px; WIDTH: 420px; FLOAT: left; MARGIN-RIGHT: 0px
}
#main_home .contentWrapper #bodyUserPass .contentUserPass H1 {
	PADDING-BOTTOM: 5px; MARGIN: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; FONT: bold 21px Arial, Helvetica, Verdana, sans-serif; COLOR: #333; PADDING-TOP: 0px
}
