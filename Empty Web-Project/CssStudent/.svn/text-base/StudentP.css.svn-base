	BORDER-BOTTOM: #17407e 1px solid; TEXT-ALIGN: left; BORDER-LEFT: #17407e 1px solid; PADDING-BOTTOM: 0px; TEXT-INDENT: 10px; MARGIN: 5px 0px 0px 10px; PADDING-LEFT: 0px; WIDTH: 290px; PADDING-RIGHT: 0px; FONT: 12px Arial, Helvetica, Verdana, sans-serif; HEIGHT: 20px; COLOR: #17407e; BORDER-TOP: #17407e 1px solid; BORDER-RIGHT: #17407e 1px solid; PADDING-TOP: 4px
}
#logo {
	PADDING-BOTTOM: 0px; PADDING-LEFT: 0px; PADDING-RIGHT: 0px; FLOAT: left; PADDING-TOP: 0px
}
#loginBackground {
	MIN-HEIGHT: 340px; WIDTH: 950px; BACKGROUND: url(CssStudent/C.jpg) no-repeat center top
}
#loginBottomRow {
	WIDTH: 950px; BACKGROUND: url(CssStudent/C4.jpg) no-repeat center top; HEIGHT: 49px; OVERFLOW: auto
}
#logonButton {
	PADDING-BOTTOM: 5px; MARGIN: 5px 0px 5px 47px; PADDING-LEFT: 47px; PADDING-RIGHT: 0px; PADDING-TOP: 5px
}
.changePasswordText {
	PADDING-BOTTOM: 0px; MARGIN: 2px 0px 0px 47px; PADDING-LEFT: 47px; PADDING-RIGHT: 0px; FONT: 10px Arial, Helvetica, Verdana, sans-serif; PADDING-TOP: 2px
}
.changePasswordText A:link {
	COLOR: #003399; TEXT-DECORATION: none
