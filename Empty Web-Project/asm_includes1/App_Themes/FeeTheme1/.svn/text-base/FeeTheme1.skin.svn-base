
<%@ Register Assembly="CommonClassLibrary" Namespace="CommonClassLibrary" TagPrefix="cc1" %>
<asp:GridView runat="server"   CssClass="paging_gridview">
  <%--<FooterStyle ForeColor="#FFFFFF" BackColor="#AAAAAA"></FooterStyle>--%>
