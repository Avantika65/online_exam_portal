/* grid */
table.paging_gridview { border-collapse: collapse; margin: 0px !important; border: #6593CF 1px solid; }
.paging_gridview td { font-size: 12px; font-family: Arial; color: #000000; cursor: default; text-align: left; }
.paging_gridview th { font-size: 12px; font-family: Arial; color:  #484848; cursor: default; text-align: left; }
table.paging_gridview a { color: #555555; text-decoration: none; }
table.paging_gridview a:hover { text-decoration: underline; cursor:pointer; }

/* header */
 tr.paging_gridview_hdr { background:#eee url("js_admin/jquery-ui-1.10.3.custom/css/custom-theme/jquery-ui-1.10.3.custom.min.css") repeat-x; }
.paging_gridview_hdr th { color:brown; font-weight: bold; border-radius:4px; text-align: left;border-top: solid 0px #cccccc; border-bottom: solid 1px #cccccc; border-left: solid 1px #cccccc; border-right: solid 1px #cccccc; padding-left: 5px; padding-right: 5px; padding-top: 2px; padding-bottom: 2px; }
.paging_gridview_hdr th a { color: #555555; text-decoration: none; }
.paging_gridview_hdr th a:hover { color: #484848; text-decoration: none; }
.paging_gridview_hdr th select { border-style:none; background:#eee url("js_admin/jquery-ui-1.10.3.custom/css/custom-theme/jquery-ui-1.10.3.custom.min.css") repeat-x; color:brown; font-weight: bold; height:27px; text-decoration: none; }
.paging_gridview_hdr th select:focus { border-style:none; background:#eee url("js_admin/jquery-ui-1.10.3.custom/css/custom-theme/jquery-ui-1.10.3.custom.min.css") repeat-x; color:brown; font-weight: bold; height:27px; text-decoration: none; }

.paging_gridview_hdr th select:option { border-style:none; background:#eee url("js_admin/jquery-ui-1.10.3.custom/css/custom-theme/jquery-ui-1.10.3.custom.min.css") repeat-x; color:brown; font-weight: bold; height:27px; text-decoration: none; }


/*Footer*/
tr.paging_gridview_Footer { background:#eee url("js_admin/jquery-ui-1.10.3.custom/css/custom-theme/jquery-ui-1.10.3.custom.min.css") repeat-x; }
.paging_gridview_Footer th { color: #555555; font-weight: normal; border-radius:4px; text-align: left;border-top: solid 1px #cccccc; border-bottom: solid 1px #cccccc; border-left: solid 1px #cccccc; border-right: solid 1px #cccccc; padding-left: 5px; padding-right: 5px; padding-top: 2px; padding-bottom: 2px; }
.paging_gridview_Footer td {color:#555555;  border-right: #cccccc 1px solid; border-top: #cccccc 1px solid; border-left: #cccccc 1px solid; border-bottom: #cccccc 1px solid; }

.paging_gridview_Footer th a { color: #484848; text-decoration: none; }
.paging_gridview_Footer th a:hover { color: #484848; text-decoration: none; }

/* item row */
tr.paging_gridview_itm { background-color: white; height:35px;}
.paging_gridview_itm td {color:#555555; font-weight:normal; font-size:12px;  border-right: #cccccc 1px solid; border-top: #cccccc 1px solid; border-left: #cccccc 1px solid; border-bottom: #cccccc 1px solid; padding-left:5px; padding-right:5px; padding-top: 2px; padding-bottom: 2px; }
.paging_gridview_itm span {color:#555555; }
.paging_gridview_itm td a { color:#5e87b0; text-decoration: none; }
 tr.paging_gridview_itm:hover { background-color:#dddddd; text-decoration: none; }

/* alternating item row */
tr.paging_gridview_aitm { background-color: #FFFFF0; height:35px;}
.paging_gridview_aitm td {color:#555555; min-height:10px; font-weight:normal; font-size:12px;  border-right: #cccccc 1px solid; border-top: #cccccc 1px solid; border-left: #cccccc 1px solid; border-bottom: #cccccc 1px solid; padding-left:5px; padding-right:5px; padding-top: 2px; padding-bottom: 2px; }
.paging_gridview_aitm span {color:#555555; }
.paging_gridview_aitm td a { color:#5e87b0; text-decoration: none; }
tr.paging_gridview_aitm:hover { background-color:#dddddd; text-decoration: none; }


