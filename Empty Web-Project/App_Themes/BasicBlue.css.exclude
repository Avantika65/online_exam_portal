.rdginputbox
 {
  COLOR: #555555;
  FONT-SIZE: 12px;
  font-family:arial ;
  font-weight:normal;
  display: block;
  width: 100%;
  height: 30px;
  padding: 6px 12px;
  
  line-height: 1.428571429;
  
  
  vertical-align: middle;
  background-color: #ffffff;
  background-image: none;
  border: 1px solid #cccccc;
  border-radius: 4px;
  font-family:arial;
  -webkit-box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  box-shadow: inset 0 1px 1px rgba(0, 0, 0, 0.075);
  -webkit-transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
  transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;

 
}

.rdginputbox:focus
{
    display: block;
    border: 1px solid skyblue;
    border-shadow:1px 1px 1px 1px outset skyblue;
    border-radius: 4px;
	TEXT-ALIGN: left; 
	BACKGROUND-POSITION: left center;
	color: #555555;
	font-size: 12px;
	FONT-WEIGHT: normal;
	font-family:arial;
	margin-left: 0px;
	cursor:hand;
}

.rdginputbox:blur
{
    display: block;
    border: 2px solid skyblue;
    border-shadow:2px 2px 2px 2px outset skyblue;
    border-radius: 4px;
	TEXT-ALIGN: left; 
	BACKGROUND-POSITION: left center;
	color: #555555;
	font-size: 14px;
	FONT-WEIGHT: normal;
	font-family:arial;
	margin-left: 0px;
	cursor:hand;
}

